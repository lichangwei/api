{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/main.ts",
    "build": "npm run build:clean && npm run build:ui && npm run build:server",
    "serve": "cross-env NODE_ENV=production pm2 .next/production-server/main.js --name api",
    "stop": "pm2 stop api",
    "build:clean": "rimraf .next",
    "build:ui": "next build",
    "build:server": "tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\""
  },
  "dependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/platform-express": "^6.5.3",
    "@next/bundle-analyzer": "9.2.2",
    "@types/express": "4.17.2",
    "@types/puppeteer": "2.0.1",
    "@types/qrcode": "1.3.4",
    "@types/request-ip": "0.0.35",
    "@zeit/next-less": "1.0.1",
    "cross-env": "^6.0.3",
    "express": "4.17.1",
    "less": "3.11.1",
    "less-plugin-autoprefix": "2.0.0",
    "nest-next": "^9.1.0",
    "next": "^9.0.3",
    "nodemon": "2.0.2",
    "puppeteer": "2.1.1",
    "qrcode": "1.4.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "2.1.3",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
