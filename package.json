{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/main.ts",
    "build": "npm run build:clean && npm run build:ui && npm run build:server",
    "serve": "cross-env NODE_ENV=production pm2 start --name api .next/production-server/main.js",
    "stop": "pm2 stop api",
    "build:clean": "rimraf .next",
    "build:ui": "next build",
    "build:server": "tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\""
  },
  "dependencies": {
    "@nestjs/common": "6.11.11",
    "@nestjs/core": "6.11.11",
    "@nestjs/platform-express": "6.11.11",
    "@nestjs/swagger": "4.3.2",
    "@next/bundle-analyzer": "9.3.0",
    "@types/d3": "5.9.2",
    "@types/jsdom": "16.2.4",
    "@zeit/next-less": "1.0.1",
    "cross-env": "7.0.2",
    "d3": "6.2.0",
    "express": "4.17.1",
    "jsdom": "16.4.0",
    "less": "3.11.1",
    "less-plugin-autoprefix": "2.0.0",
    "nest-next": "9.2.0",
    "next": "9.3.0",
    "nodemon": "2.0.2",
    "puppeteer": "2.1.1",
    "qrcode": "1.4.4",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "2.1.3",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0",
    "tslint-react-hooks": "2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@types/express": "4.17.3",
    "@types/node": "13.9.1",
    "@types/puppeteer": "2.0.1",
    "@types/qrcode": "1.3.4",
    "@types/react": "16.9.23",
    "@types/request-ip": "0.0.35",
    "rimraf": "3.0.2",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  }
}
